<%- include ("partials/header") -%>
<br><br>
<section style="padding: 2% 7%">
    <h1><%= test.name %></h1>
    <% var date = new Date(); %>
    <% var strDate = date.getFullYear().toString() + '/' + (date.getMonth() + 1).toString() + '/' + date.getDate().toString(); %>  
    <p>Test id: 00<%= test.id %> <em style="margin-left: 4%;">Created on <%= strDate %></em></p>
    <br><br>
    <h4>Instructions</h4>
    <p><%= test.instructions %> </p>
</section>


<%- include ("partials/footer") -%>